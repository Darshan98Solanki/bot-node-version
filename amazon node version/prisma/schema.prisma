generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  name         String
  email        String
  location     String
  sessionToken String?
  accessToken  String?
  bbCandidateId  String?
}
